            android:name=".MoodClasses.MoodActivity"
            android:exported="false" />
        <activity
            android:name=".workoutClasses.WorkoutActivity"
            android:exported="false" />
        <activity
            android:name="notesClasses.NotesActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".weightClasses.WeightActivity" />
        <activity android:name=".foodactivityclasses.FoodActivity" />
