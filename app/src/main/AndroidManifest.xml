<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.healthyorg.android.healthyapp">

    <application
        android:name=".HealthyAppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthyApp">
        <activity
            android:name=".SleepActivity"
            android:exported="false" />
        <activity
            android:name=".QuotesActivity"
            android:exported="false" />
        <activity
            android:name=".MoodActivity"
            android:exported="false" />
        <activity
            android:name=".WorkoutActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WeightActivity"/>
        <activity android:name=".FoodActivity"/>
        <activity android:name=".GoalsActivity"/>
    </application>

</manifest>